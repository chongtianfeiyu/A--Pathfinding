package pathfinding{	public interface IGraph	{				/**		*	Add a node to the graph.		*/		function addNode(node:INode):void;				/**		*	Create a relationship between two nodes, and add the cost for moving from		*	one node to the other.		*/		function addNeighbour(fromNode:INode, toNode:INode, cost:Number):void;		/**		*	Get an Array of INode instances which are neighbours of node.		*/		function getNeighbours(node:INode):Array;				function getCost(fromNode:INode, toNode:INode):Number;			}}